    }

    .visualizer-container {
        flex: 1;
        height: 100%;
        display: flex;
        align-items: center;
        justify-content: center;
        overflow: hidden;
        width: 100%;
    }

    .recording-timer {
        font-family: 'Inter', monospace;
        font-size: 0.9rem;
        color: var(--color-text-accent);
        font-weight: 600;
        min-width: 45px;
        text-align: right;
    }

    @keyframes fadeIn {
        from {
            opacity: 0;
            transform: translateY(5px);
        }

        to {
            opacity: 1;
            transform: translateY(0);
        }
    }

    /* Voice Preview UI */
    .voice-preview {
        flex: 1;
        display: flex;
        align-items: center;
        gap: var(--spacing-md);
        background: var(--color-surface);
        border: 1px solid var(--color-border-focus);
        border-radius: var(--radius-lg);
        padding: 0 var(--spacing-md);
        height: 50px;
        animation: fadeIn 0.3s ease;
    }

    .btn-icon-small {
        background: transparent;
        border: none;
        color: var(--color-text-secondary);
        cursor: pointer;
        padding: 4px;
        display: flex;
        align-items: center;
        justify-content: center;
        border-radius: 50%;
        transition: all var(--transition-base);
    }

    .btn-icon-small:hover {
        background: rgba(255, 255, 255, 0.1);
        color: var(--color-text-primary);
    }

    .btn-icon-small.danger:hover {
        background: rgba(239, 68, 68, 0.2);
        color: #ef4444;
    }

    .btn-icon-small.success:hover {
        background: rgba(16, 185, 129, 0.2);
        color: #10b981;
    }

    .preview-waveform {
        flex: 1;
        display: flex;
        align-items: center;
        justify-content: center;
        gap: 3px;
        height: 24px;
    }

    .preview-bar {
        width: 3px;
        background: var(--color-text-accent);
        border-radius: 2px;
        animation: waveform 1s ease-in-out infinite;
    }

    .preview-bar:nth-child(1) {
        height: 12px;
        animation-delay: 0.0s;
    }

    .preview-bar:nth-child(2) {
        height: 20px;
        animation-delay: 0.1s;
    }

    .preview-bar:nth-child(3) {
        height: 16px;
        animation-delay: 0.2s;
    }

    .preview-bar:nth-child(4) {
        height: 22px;
        animation-delay: 0.3s;
    }

    .preview-bar:nth-child(5) {
        height: 14px;
        animation-delay: 0.4s;
    }

    /* Pause animation when not playing */
    .voice-preview:not(.playing) .preview-bar {
        animation-play-state: paused;
        height: 4px;
        /* Collapse when paused */
        transition: height 0.3s ease;
    }

    .voice-preview.playing .preview-bar {
        animation-play-state: running;
    }

    @keyframes waveform {

        0%,
        100% {
            transform: scaleY(1);
        }

        50% {
            transform: scaleY(1.5);
        }
    }

    .preview-actions {
        display: flex;
        gap: var(--spacing-sm);
        border-left: 1px solid var(--color-border);
        padding-left: var(--spacing-sm);
    }

    .preview-content-wrapper {
        flex: 1;
        display: flex;
        flex-direction: column;
        justify-content: center;
        gap: 4px;
        overflow: hidden;
    }

    .preview-transcript {
        font-size: 0.8rem;
        color: var(--color-text-secondary);
        white-space: nowrap;
        overflow: hidden;
        text-overflow: ellipsis;
        max-width: 100%;
    }